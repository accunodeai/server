{
  "family": "accunode-worker",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "arn:aws:iam::461962182774:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "accunode-worker",
      "image": "461962182774.dkr.ecr.us-east-1.amazonaws.com/accunode:testing-image",
      "essential": true,
      "command": [
        "celery",
        "-A",
        "app.workers.celery_app",
        "worker",
        "--loglevel=info",
        "--concurrency=2",
        "--queues=high_priority,medium_priority,low_priority"
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/accunode-worker",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "REDIS_HOST",
          "value": "accunode-redis.d9avr2.0001.use1.cache.amazonaws.com"
        },
        {
          "name": "REDIS_DB",
          "value": "0"
        },
        {
          "name": "PYTHONPATH",
          "value": "/app"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        },
        {
          "name": "ENABLE_ML_CACHE",
          "value": "true"
        },
        {
          "name": "ACCESS_TOKEN_EXPIRE_MINUTES",
          "value": "60"
        },
        {
          "name": "PORT",
          "value": "8000"
        },
        {
          "name": "ENVIRONMENT",
          "value": "production"
        },
        {
          "name": "REDISUSER",
          "value": ""
        },
        {
          "name": "SECRET_KEY",
          "value": "accunode-production-secret-key-2024-secure"
        },
        {
          "name": "DEBUG",
          "value": "false"
        },
        {
          "name": "CORS_ORIGIN",
          "value": "https://accunode.ai,https://client-eta-sepia.vercel.app"
        },
        {
          "name": "DATABASE_URL",
          "value": "postgresql://accunode_admin:AccuNode2024!SecurePass@accunode-postgres.ck36iu4u6mpj.us-east-1.rds.amazonaws.com:5432/postgres"
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": "1"
        },
        {
          "name": "REDIS_PORT",
          "value": "6379"
        },
        {
          "name": "MODEL_PATH",
          "value": "./models/"
        },
        {
          "name": "REDIS_PASSWORD",
          "value": ""
        },
        {
          "name": "LOG_LEVEL",
          "value": "INFO"
        },
        {
          "name": "REDIS_URL",
          "value": "redis://accunode-redis.d9avr2.0001.use1.cache.amazonaws.com:6379"
        }
      ]
    }
  ]
}
